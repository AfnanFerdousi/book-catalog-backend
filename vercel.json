{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/server.js"
        }
    ],
    "projectSettings": {
        "name": "book-catalog",
        "region": "asia-south1",
        "runtime": "node@14"
    }
}